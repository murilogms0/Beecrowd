casos = int(input(''))

for _ in range(casos):
    B = int(input(''))
    B_original = B
    E = int(input(''))
    
    while True:
        if B < 1:
            B = int(input(''))
        elif B > E:
            E = int(input(''))
        else:
            break

for i in range(B, E + 1):
    print(i, end='')

for i in range(E - 1, B - 1, -1):
    print(i, end='')

print()
